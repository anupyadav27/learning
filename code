aws dynamodb create-table \
    --table-name CloudNodes \
    --attribute-definitions AttributeName=node_id,AttributeType=S \
    --key-schema AttributeName=node_id,KeyType=HASH \
    --billing-mode PAY_PER_REQUEST


aws dynamodb create-table \
    --table-name CloudRelations \
    --attribute-definitions AttributeName=relation_id,AttributeType=S \
    --key-schema AttributeName=relation_id,KeyType=HASH \
    --billing-mode PAY_PER_REQUEST
